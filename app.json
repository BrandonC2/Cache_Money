{
  "expo": {
    "name": "LetMeCook",
    "slug": "LetMeCook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/ye.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/ye.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/ye.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./app/assets/favicona.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
